<section class="hero grid">
  <div class="top_row left_col">
    <h1>Console.Blog</h1>
    <p>Blog for Developers</p>
  </div>
  <div class="bottom_row right_col">
    <p>Lorem ipsum dolor sit amet consectetur adipisicing elit. Suscipit, ipsum!</p>
  </div>
</section>
<section>
  <div>
    <% articles.forEach(element => { %>
      <a href="<%= element.url %>">
        <h3>s<%= element.name%><h3>
        <img src="<%= element.provider[0].image.thumbnail.contentUrl %>" alt="">
      </a>
    <% }) %>
  </div>
</section>
<section class="latest">
  <div>
    <% posts.forEach(element => { %>
      <a href="/post/<%= element._id %>">
        <h3><%= element.title%><h3>
      </a>
    <% }) %>
  </div>
</section>
<section class="todays_method">
  <h4><%= method %></h4>
</section>

